echo "Test X: (shortest distance, number of paths)"
echo "TEST 1: (2,2)"
./greedy_robot 1 3 4 4 3
echo "TEST 2: (Already there)"
./greedy_robot 2 1 1 1 1
echo "TEST 3: (NP, 0)"
./greedy_robot 1 1 1 5 1
echo "TEST 4: Simple Diagonal Movement"
./greedy_robot 2 0 0 1 1
echo "TEST 5: Long Single Direction"
./greedy_robot 3 0 0 0 5
echo "TEST 6: Multiple Valid Paths"
./greedy_robot 3 0 0 2 2
echo "TEST 7: Starting Near Treasure"
./greedy_robot 4 9 9 10 10
echo "TEST 8: Negative Coordinates"
./greedy_robot 2 -2 -2 0 0
echo "TEST 9: Zero Movement Required"
./greedy_robot 3 5 5 5 5
echo "TEST 10: Exact Max Distance Needed"
./greedy_robot 4 0 0 4 4
echo "TEST 11: Longer distance between points"
./greedy_robot 5 10 15 22 19





